<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane disable="true" minHeight="120.0" minWidth="280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <StackPane minWidth="127.0" BorderPane.alignment="CENTER">
         <children>
            <Label minWidth="127.0" text="Code Calibaration">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets top="10.0" />
         </padding>
      </StackPane>
   </top>
   <bottom>
      <HBox alignment="CENTER" minHeight="50.0" minWidth="280.0" spacing="40.0">
         <children>
            <Button minHeight="40.0" minWidth="120.0" mnemonicParsing="false" text="Random Code" />
            <Button minHeight="40.0" minWidth="120.0" mnemonicParsing="false" text="Set Code" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
